{
	"Central Image": {
		"prefix": ["cci"],
		"body": [
			"\\begin{center}",
			"img ${1:name} ${2:size}",
			"_crimg \"${1:name}\" ${3:page}",
			"\\end{center}",
			"$0"
		],
		"description": "A centered image"
	},
	"LMD-Start": {
		"prefix": ["lmd"],
		"body": [
			"\\title{${1:title}}",
			"\\author{${2:author}}",
			"",
			"% preamble",
			"",
			"",
			"_startdocument",
			"",
			"\\maketitle",
			"\\tableofcontents",
			"\\pagebreak",
			"",
			"_setpdf \"${3:pdfpath}\"",
			"",
			"##### Chapter"
		]
	}
}
