{
	"makros": {
		"startdocument": "startdocument"
	},
	"noteStarts": [
		",",
		".",
		"+",
		"-",
		"*",
		"eg",
		"\"",
		"!",
		"=>",
		"land",
		"lor"
	],
	"lineStarts": {
		"makro": "^_",
		"section": "^#",
		"def": "^\t*>",
		"note": "^\t*(,|\\.|\\+|-|\\*|eg|\"|!|=>|land|lor)",
		"math": "^\t*\\$\\$",
		"image": "img \\w* [0-9](\\.[0-9]+)?",
		"comment": "^\t*%"
	}
}
