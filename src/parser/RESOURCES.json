{
	"makros": {
		"startdocument": "startdocument",
		"setpdf": {
			"name": "setpdf",
			"args": ["string"]
		},
		"crimg": {
			"name": "crimg",
			"args": ["string", "int"]
		}
	},
	"makroArgRegex": {
		"string": "\".+\"",
		"int": "(0|[1-9]\\d*)"
	},
	"noteStarts": [
		",",
		".",
		"+",
		"-",
		"*",
		"eg",
		"\"",
		"!",
		"=>",
		"land",
		"lor",
		"["
	],
	"lineStarts": {
		"makro": "^_",
		"section": "^#",
		"def": "^\t*>",
		"note": "^\t*(,|\\.|\\+|-|\\*|eg|\"|!|=>|land|lor|\\[)",
		"math": "^\t*\\$\\$",
		"image": "img \\w* [0-9](\\.[0-9]+)?",
		"comment": "^\t*%"
	}
}
