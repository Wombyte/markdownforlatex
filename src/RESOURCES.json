{
	"makros": {
		"startdocument": "startdocument",
		"setpdf": {
			"name": "setpdf",
			"args": ["string"]
		},
		"crimg": {
			"name": "crimg",
			"args": ["string", "int"]
		},
		"cwd": {
			"name": "cwd",
			"args": ["string"]
		}
	},
	"makroArgRegex": {
		"string": "\".+\"",
		"int": "(0|[1-9]\\d*)"
	},
	"noteStarts": [",", ".", "+", "-", "*", "eg", "\"", "!", "=>", "&", "|", "["],
	"lineStarts": {
		"makro": "^\t*_",
		"section": "^#",
		"def": "^\t*>",
		"note": "^\t*(,|\\.|\\+|-|\\*|eg|\"|!|=>|&|\\||\\[)",
		"math": "^\t*\\$\\$",
		"image": "img \\w* [0-9](\\.[0-9]+)?",
		"comment": "^\t*%"
	}
}
